# Ejemplo de variables de entorno para la migración
# Rellena con los valores reales y renómbralo a .env (NO subir .env a git)

# Fuente (origen)
SOURCE_DB_URL=postgresql://user:password@host:5432/source_db

# Destino (target)
TARGET_DB_URL=postgresql://user:password@host:5432/target_db

# Opciones
LOG_LEVEL=INFO
DRY_RUN=true
